</audio>
endsnippet

snippet 'iframe' 'Inserir um iframe'
<iframe src="${1:https://example.com}" width="${2:600}" height="${3:400}" frameborder="${4:0}"></iframe>
endsnippet

snippet 'meta-viewport' 'Meta tag para responsividade'
<meta name="viewport" content="width=device-width, initial-scale=1.0">
endsnippet

snippet 'div' 'Div básica'
