snippet 'html5' 'Estrutura básica do HTML5' 
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>${1:Document}</title>
</head>
<body>
    $2
</body>
</html>
endsnippet

snippet 'link' 'Adicionar um link' 
<a href="${1:https://example.com}" target="${2:_blank}">${3:Texto do link}</a>
endsnippet

snippet 'img' 'Adicionar uma imagem' 
<img src="${1:caminho/da/imagem.jpg}" alt="${2:Descrição da imagem}" width="${3:300}" height="${4:200}">
endsnippet

snippet 'form' 'Estrutura básica de um formulário' 
<form action="${1:/submit}" method="${2:post}">
    <label for="${3:id}">${4:Texto do campo}</label>
    <input type="${5:text}" id="${3:id}" name="${6:nome}">
    <button type="submit">${7:Enviar}</button>
</form>
endsnippet

snippet 'table' 'Tabela básica' 
<table border="${1:1}">
    <thead>
        <tr>
            <th>${2:Cabeçalho 1}</th>
            <th>${3:Cabeçalho 2}</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>${4:Valor 1}</td>
            <td>${5:Valor 2}</td>
        </tr>
    </tbody>
</table>
endsnippet

snippet 'list' 'Lista não ordenada'
<ul>
    <li>${1:Item 1}</li>
    <li>${2:Item 2}</li>
    <li>${3:Item 3}</li>
</ul>
endsnippet

snippet 'list-ordered' 'Lista ordenada'
<ol>
    <li>${1:Item 1}</li>
    <li>${2:Item 2}</li>
    <li>${3:Item 3}</li>
</ol>
endsnippet

snippet 'button' 'Botão básico'
<button type="${1:button}">${2:Clique aqui}</button>
endsnippet

snippet 'video' 'Incluir um vídeo'
<video controls width="${1:600}" height="${2:400}">
    <source src="${3:caminho/do/video.mp4}" type="video/mp4">
    Seu navegador não suporta vídeos HTML5.
</video>
endsnippet

snippet 'audio' 'Incluir um áudio'
<audio controls>
    <source src="${1:caminho/do/audio.mp3}" type="audio/mp3">
